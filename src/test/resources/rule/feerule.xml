	</rule>
	<rule id="step7" exclusive="true">
		<condition><![CDATA[salary>58500 && salary<=83500]]></condition>
		<action><![CDATA[fee=(salary-3500)*0.35-5505]]></action>
	</rule>
	<rule id="step8" exclusive="true">
